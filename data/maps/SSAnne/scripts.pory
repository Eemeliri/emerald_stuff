script ssAnneAquaGrunt {
    lock
    faceplayer
    delay(60)
    applymovement(10, aquaGruntSpinAnne)
    waitmovement(10)
    removeobject(10)
    setvar(VAR_OBJ_GFX_ID_0, OBJ_EVENT_GFX_MAGMA_MEMBER_F)
    addobject(10)
    applymovement(10, aquaGruntSpinAnne2)
    waitmovement(10)
    faceplayer
    msgbox("hi")
    release
    end
}

movement aquaGruntSpinAnne {
    face_up
	delay_2
	face_left
	delay_2
	face_down
	delay_2
	face_right
	delay_2
    face_up
	delay_2
	face_left
	delay_2
	step_end
}

movement aquaGruntSpinAnne2 {
    face_down
	delay_2
	face_right
	delay_2
    face_up
	delay_2
	face_left
	delay_2
	face_down
	delay_2
	face_right
	delay_2
	step_end
}