script harborTourist {
    lock
    faceplayer
    msgbox(format("I'm currently visiting Hoenn with my girlfriend. We've lived in Alola for our whole lives, but decided recently to start travelling the world."))
    msgbox(format("Have you visited any other regions?"))
    waitmessage
    release
    end
}

